    "@angular/platform-browser": "^13.4.0",
    "@angular/platform-browser-dynamic": "^13.4.0",
    "@angular/router": "^13.4.0",
    "core-js": "^3.26.1",
    "rxjs": "~7.6.0",
    "tributejs": "^5.1.3",
    "tslib": "^2.4.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13",
    "@angular/cli": "^13",
    "@angular/compiler-cli": "^13",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
